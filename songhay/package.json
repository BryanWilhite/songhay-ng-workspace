{
  "name": "songhay-ng-workspace",
  "version": "0.0.0",
  "scripts": {
    "e2e": "ng e2e",
    "ng": "ng",
    "build:client": "ng build client",
    "build:core": "ng build @songhay/core",
    "build:index": "ng build @songhay/index",
    "build:player-video-you-tube": "ng build @songhay/player-video-you-tube",
    "build-prod:core": "ng build --prod @songhay/core",
    "build-prod:index": "ng build --prod @songhay/index",
    "build-prod:player-video-you-tube": "ng build --prod @songhay/player-video-you-tube",
    "build": "npm run build:core && npm run build:index && npm run build:player-video-you-tube && ng build client",
    "build-prod": "npm run build-prod:core && npm run build-prod:index && npm run build-prod:player-video-you-tube && ng build client",
    "lint": "ng lint",
    "pack:core": "npm pack dist/songhay/core",
    "pack:index": "npm pack dist/songhay/index",
    "pack:player-video-you-tube": "npm pack dist/songhay/player-video-you-tube",
    "publish:core": "npm publish dist/songhay/core --access public",
    "publish:index": "npm publish dist/songhay/index --access public",
    "publish:player-video-you-tube": "npm publish dist/songhay/player-video-you-tube --access public",
    "release:core": "npm run build-prod:core && npm run pack:core && npm run publish:core",
    "release:index": "npm run build-prod:index && npm run pack:index && npm run publish:index",
    "release:player-video-you-tube": "npm run build-prod:player-video-you-tube && npm run pack:player-video-you-tube && npm run publish:player-video-you-tube",
    "start": "ng serve --open",
    "test:core": "ng test --project=@songhay/core",
    "test:index": "ng test --project=@songhay/index",
    "test:player-video-you-tube": "ng test --project=@songhay/player-video-you-tube"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BryanWilhite/songhay-ng-workspace.git"
  },
  "keywords": [],
  "author": {
    "name": "Bryan D. Wilhite",
    "email": "rasx@songhaysystem.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BryanWilhite/songhay-ng-workspace/issues"
  },
  "homepage": "https://github.com/BryanWilhite/songhay-ng-workspace#readme",
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^10.2.7",
    "@angular/cdk-experimental": "^10.2.7",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^11.2.14",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@types/numeral": "0.0.25",
    "core-js": "^2.6.9",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ngx-pagination": "^5.1.1",
    "normalize.css": "^8.0.1",
    "numeral": "^2.0.6",
    "rxjs": "~6.5.4",
    "songhay": "^0.1.17",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.14",
    "@angular-devkit/core": "^11.2.14",
    "@angular/cli": "^11.2.14",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.170",
    "@types/node": "^12.19.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "^4.1.6"
  }
}