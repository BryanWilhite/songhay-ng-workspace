{
    "name": "songhay-ng-workspace",
    "version": "0.0.0",
    "scripts": {
        "e2e": "ng e2e",
        "ng": "ng",
        "build:client": "ng build client",
        "build:core": "ng build @songhay/core",
        "build:index": "ng build @songhay/index",
        "build:player-video-you-tube": "ng build @songhay/player-video-you-tube",
        "build": "npm run build:core && npm run build:index && npm run build:player-video-you-tube && ng build client",
        "lint": "ng lint",
        "pack:core": "npm pack dist/songhay/core",
        "pack:index": "npm pack dist/songhay/index",
        "pack:player-video-you-tube": "npm pack dist/songhay/player-video-you-tube",
        "publish:core": "npm publish dist/songhay/core --access public",
        "publish:index": "npm publish dist/songhay/index --access public",
        "publish:player-video-you-tube": "npm publish dist/songhay/player-video-you-tube --access public",
        "release:core": "npm run build:core && npm run pack:core && npm run publish:core",
        "release:index": "npm run build:index && npm run pack:index && npm run publish:index",
        "release:player-video-you-tube": "npm run build:player-video-you-tube && npm run pack:player-video-you-tube && npm run publish:player-video-you-tube",
        "start": "ng serve --open",
        "test:core": "ng test --project=@songhay/core",
        "test:index": "ng test --project=@songhay/index",
        "test:player-video-you-tube": "ng test --project=@songhay/player-video-you-tube"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BryanWilhite/songhay-ng-workspace.git"
    },
    "keywords": [],
    "author": {
        "name": "Bryan D. Wilhite",
        "email": "rasx@songhaysystem.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/BryanWilhite/songhay-ng-workspace/issues"
    },
    "homepage": "https://github.com/BryanWilhite/songhay-ng-workspace#readme",
    "dependencies": {
        "@angular/animations": "^7.2.15",
        "@angular/cdk": "^7.3.7",
        "@angular/cdk-experimental": "^7.3.7",
        "@angular/common": "^7.2.15",
        "@angular/compiler": "^7.2.15",
        "@angular/core": "^7.2.15",
        "@angular/flex-layout": "^7.0.0-beta.24",
        "@angular/forms": "^7.2.15",
        "@angular/http": "^7.2.15",
        "@angular/material": "^7.3.7",
        "@angular/platform-browser": "^7.2.15",
        "@angular/platform-browser-dynamic": "^7.2.15",
        "@angular/router": "^7.2.15",
        "@types/numeral": "0.0.25",
        "core-js": "^2.6.9",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "ngx-pagination": "^3.3.1",
        "normalize.css": "^8.0.1",
        "numeral": "^2.0.6",
        "rxjs": "~6.3.3",
        "songhay": "^0.1.14",
        "web-animations-js": "^2.3.2",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.901.0",
        "@angular-devkit/build-ng-packagr": "^0.901.0",
        "@angular/cli": "^7.3.9",
        "@angular/compiler-cli": "^7.2.15",
        "@angular/language-service": "^7.2.15",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/lodash": "^4.14.136",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.1.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^2.1.0",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^4.7.1",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tsickle": "^0.38.1",
        "tslib": "^1.10.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2"
    }
}
